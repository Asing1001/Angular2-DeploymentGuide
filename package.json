{
    "name": "angular2-quickstart",
    "version": "1.0.0",
    "scripts": {
        "start": "npm run build && node server.js",
        "dev": "concurrently \"npm run tsc:w\" \"npm run nodemon\" \"npm run webpack\"",
        "webpack": "webpack-dev-server --inline --progress --port 3002",
        "nodemon": "nodemon server.js",
        "tsc:w": "tsc -w server.ts",
        "build": "tsc server.ts && rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail"
    },
    "license": "ISC",
    "dependencies": {
        "@angular/common": "2.0.0",
        "@angular/compiler": "2.0.0",
        "@angular/core": "2.0.0",
        "@angular/forms": "2.0.0",
        "@angular/http": "2.0.0",
        "@angular/platform-browser": "2.0.0",
        "@angular/platform-browser-dynamic": "2.0.0",
        "@angular/router": "3.0.0",
        "@angular/upgrade": "2.0.0",
        "@types/body-parser": "0.0.33",
        "angular2-toaster": "^1.0.1",
        "body-parser": "^1.15.2",
        "bootstrap": "^3.3.7",
        "bootstrap-material-design": "^0.5.10",
        "core-js": "^2.4.1",
        "express": "^4.14.0",
        "fs-extra": "^0.30.0",
        "jquery": "^3.1.1",
        "moment": "^2.15.1",
        "reflect-metadata": "^0.1.3",
        "rxjs": "5.0.0-beta.12",
        "systemjs": "0.19.27",
        "zone.js": "^0.6.23"
    },
    "devDependencies": {
        "@types/express": "^4.0.33",
        "@types/fs-extra": "0.0.34",
        "@types/moment": "^2.13.0",
        "@types/node": "^6.0.40",
        "angular2-template-loader": "^0.4.0",
        "concurrently": "^2.2.0",
        "copy-webpack-plugin": "^3.0.1",
        "css-loader": "^0.23.1",
        "extract-text-webpack-plugin": "^1.0.1",
        "file-loader": "^0.8.5",
        "html-loader": "^0.4.3",
        "html-webpack-plugin": "^2.15.0",
        "nodemon": "^1.10.2",
        "rimraf": "^2.5.4",
        "style-loader": "^0.13.1",
        "ts-loader": "^2.0.3",
        "typescript": "^2.2.2",
        "webpack": "^1.13.2",
        "webpack-dev-server": "^1.16.1",
        "webpack-merge": "^0.14.1"
    }
}